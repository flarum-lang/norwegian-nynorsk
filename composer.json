{
	"name": "flarum-lang/norwegian-nynorsk",
	"type": "flarum-extension",
	"description": "Norwegian Nynorsk language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/norwegian-nynorsk",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"nn-locale",
		"norwegian-nynorsk-locale",
		"norwegian-nynorsk"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/norwegian-nynorsk/issues",
		"source": "https://github.com/flarum-lang/norwegian-nynorsk"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Norwegian Nynorsk",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
